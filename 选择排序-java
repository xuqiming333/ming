public class XuanZePaiXu {
   public static void main(String[] args) {
	   int arr[]=new int[]{9,5,3,8,4,2,1,6,7};
		for(int i=0;i<arr.length;i++){
			for(int j=i;j<arr.length;j++){
				if(arr[i]>arr[j]){
					int temp=arr[i];
					arr[i]=arr[j];
					arr[j]=temp;
				}
			}
		}
	   System.out.println(Arrays.toString(arr));
}
